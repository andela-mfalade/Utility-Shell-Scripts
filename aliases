alias gl='git log --oneline'
alias fixup='git commit --fixup'
alias yolo='git push origin HEAD'
alias r='source ~/.profile'
alias getupdates='git fetch && git rebase origin/master'
alias aliases='vi ~/.dotfiles/.aliases'
alias legacy='smarthost && cd client/legacy'
alias next='smarthost && cd client/next'

