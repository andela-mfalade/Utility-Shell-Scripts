{
  "workbench.colorTheme": "x3-alpha",
  "workbench.iconTheme": "material-icon-theme",
  "window.zoomLevel": -1,
  "files.autoSave": "onFocusChange",
  "editor.tabSize": 2,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "git.ignoreLimitWarning": true,
  "prettier.tabWidth": 2,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "prettier.ignorePath": "client/.prettierignore",
  "[yaml]": {
    "editor.formatOnSave": false
  },
  "[json]": {
    "editor.formatOnSave": false
  },
  "[scss]": {
    "editor.formatOnSave": false
  },
  "editor.cursorSmoothCaretAnimation": true,
  "html.format.enable": false,
  "peacock.favoriteColors": [{
      "name": "Angular Red",
      "value": "#b52e31"
    },
    {
      "name": "Auth0 Orange",
      "value": "#eb5424"
    },
    {
      "name": "Azure Blue",
      "value": "#007fff"
    },
    {
      "name": "Gatsby Purple",
      "value": "#639"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#00b3e6"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    }
  ],
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "files.associations": {
    "*.js": "javascriptreact"
  },
  "path-autocomplete.pathMappings": {
    "components": "${folder}/src/app/components",
    "layouts": "${folder}/src/app/layouts",
    "views": "${folder}/src/app/views",
    "routes": "${folder}/src/app/routes",
    "helpers": "${folder}/src/app/helpers",
    "services": "${folder}/src/app/services",
  }
}
